{
  "name" : "whatsapp web client smiley support",
  "author":   "Furkan BAÅžARAN",
  "short_name": "whatsapp smiley",
  "description" : "convert keyboard shortcuts as whatsapp smiley",
  "version" : "0.9.3",
  "permissions": ["http://web.whatsapp.com/*", "https://web.whatsapp.com/*"],
  "manifest_version" : 2,
  "homepage_url":"http://frknbasaran.github.io/whatsapp-smiley-support",
  "icons": {
    "16": "assert/32-icon.png",
    "48": "assert/64-icon.png",
    "128": "assert/128-icon.png"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://web.whatsapp.com/*",
        "https://web.whatsapp.com/*"
      ],
      "js" : [
        "lib/jquery-2.1.3.min.js",
        "src/helper.js"
      ]
    }
  ]
}
